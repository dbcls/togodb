<%# if @db.name == 'antmossdb' %>
<% if @use_metastanza %>
(function() {
    const quickBrowseDiv = document.getElementById("<%= quickbrowse_data_elem_id(@table) %>");
    quickBrowseDiv.innerHTML = "<%== j @metastanza_tag %>";
})();
<% else %>
  <% if true %>
    $("div#<%= quickbrowse_data_elem_id(@table) %> > div#togodb-quickbrowse-body").html('<iframe id="qb-ifm" src="<%== j @src %>" onload="resize_qb()"></iframe>');
  <% else %>
    $("div#<%= quickbrowse_data_elem_id(@table) %> > div#togodb-quickbrowse-body").html("<%== j @html %>");
  <% end %>
<% end %>

$(function() {
    const tabManager = new TabManager();
    const tabLoginSocialAccount = new Tab("tab_login_sns_account", tabManager);
    // const tabLoginOpenid = new Tab("tab_login_openid", tabManager);
    const tabLoginAdmin = new Tab("tab_login_admin", tabManager);

    tabManager.addTab(tabLoginSocialAccount);
    // tabManager.addTab(tabLoginOpenid);
    tabManager.addTab(tabLoginAdmin);
    <% if flash[:account_login_error] %>
      $("#tab_login_admin").addClass("active");
      $("#tab_login_sns_account_contents").hide();
    <% else %>
      $("#tab_login_sns_account_contents").addClass("active");
      $("#tab_login_admin_contents").hide();
    <% end %>
});
